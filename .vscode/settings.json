{
    "cSpell.ignoreWords": [
        "j",
        "mblockplaceholder",
        "synt"
    ],
    "eslint.validate": [
        "javascript",
        "typescript",
        "javascriptreact",
        "typescriptreact",
        "html"
    ],
    "eslint.options": {
        "extensions": [
            ".js",
            ".ts",
            "html"
        ]
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
}
